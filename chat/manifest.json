{
    "display_information": {
        "name": "Tom",
        "description": "Chessboard Tracker",
        "background_color": "#389925"
    },
    "features": {
        "bot_user": {
            "display_name": "Tom",
            "always_online": false
        },
        "slash_commands": [
            {
                "command": "/hello",
                "description": "Greeting",
                "should_escape": false
            },
            {
                "command": "/goodbye",
                "description": "Farewell",
                "should_escape": false
            },
            {
                "command": "/chess",
                "description": "Chess Bot",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "commands",
                "chat:write",
                "files:read",
                "files:write"
            ]
        }
    },
    "settings": {
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}
